	</rdf:Description>

	<rdf:Description rdf:about="https://purl.org/pe-repo/concytec/equipamiento#centrifugadoras_pref_label_es">
		<skosxl:literalForm xml:lang="es">Centrifugadoras</skosxl:literalForm>
	</rdf:Description>
	

	
	

