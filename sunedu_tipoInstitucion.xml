	</rdf:Description>

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#04_pref_label_es">
		<rdf:type rdf:resource="http://www.w3.org/2008/05/skos-xl#Label"/>
	</rdf:Description>

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#04_pref_label_es">
		<skosxl:literalForm xml:lang="es">Instituto de Educación Superior Tecnológica (IEST)</skosxl:literalForm>
