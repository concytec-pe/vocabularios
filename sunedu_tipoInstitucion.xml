	</rdf:Description>

	   
	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03">
		<skos:topConceptOf rdf:resource="http://purl.org/pe-repo/sunedu/tipoInstitucion"/>
	</rdf:Description>
	

	
	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03">
		<skos:definition rdf:resource="http://purl.org/pe-repo/sunedu/tipoInstitucion#03_def_es"/>
	</rdf:Description>

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03_def_es">
		<rdf:value xml:lang="es">La Educación Técnico-Productiva es una forma de educación orientada a la adquisición y desarrollo de competencias laborales y empresariales en una perspectiva de desarrollo sostenible, competitivo y humano, así como a la promoción de la cultura innovadora que responda a la demanda del sector productivo y a los avances de la tecnología, del desarrollo local, regional y nacional, así como a las necesidades educativas de los estudiantes en sus respectivos entornos.</rdf:value>
	</rdf:Description>
	

	
	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03">
		<skosxl:prefLabel rdf:resource="http://purl.org/pe-repo/sunedu/tipoInstitucion#03_pref_label_es"/>
	</rdf:Description>

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03_pref_label_es">
		<rdf:type rdf:resource="http://www.w3.org/2008/05/skos-xl#Label"/>
	</rdf:Description>

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03_pref_label_es">
		<skosxl:literalForm xml:lang="es">Centro de Educación Técnico Productiva (CETPRO)</skosxl:literalForm>
	</rdf:Description>
	

	
	

	<!-- Instituto de Educación Superior Tecnológica (IEST)           -->
	<!-- http://purl.org/pe-repo/sunedu/tipoInstitucion#04            -->

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#04">
