	<!-- Centro de Educación Técnico Productiva (CETPRO)              -->
	<!-- http://purl.org/pe-repo/sunedu/tipoInstitucion#03            -->

	<rdf:Description rdf:about="http://purl.org/pe-repo/sunedu/tipoInstitucion#03">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
