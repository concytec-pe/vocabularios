	<rdf:Description rdf:about="https://purl.org/pe-repo/renati/level#tituloProfesional">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	</rdf:Description>

	<rdf:Description rdf:about="https://purl.org/pe-repo/renati/level#tituloProfesional">
